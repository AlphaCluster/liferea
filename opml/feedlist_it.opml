<?xml version="1.0"?>
<opml version="1.0">
<head>
	<title>Elenco notiziari predefiniti</title>
</head>
<body>
        <outline text="Notiziari d'esempio">
                <outline text="Notizie dall'Italia">
                        <outline text="afNews" htmlUrl="http://www.afnews.info/" xmlUrl="http://www.afnews.info/wordpress/feed/" />
